    "@types/cheerio": "0.22.21",
    "@types/jest": "26.0.8",
    "@types/node": "12.12.53",
    "@types/request-promise": "4.1.46",
    "babel-jest": "26.2.1",
    "jest": "26.2.1",
    "jest-junit-reporter": "1.1.0",
